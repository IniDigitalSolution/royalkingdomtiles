{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/Home%20Page%20Slide/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/Products/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    },
    {
      "source": "/Fonts/(.*)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ],
  "rewrites": [
    { "source": "/about-cool-roof-tiles-dealer-chennai", "destination": "/about-cool-roof-tiles-dealer-chennai.html" },
    { "source": "/roof-tiles-products-chennai", "destination": "/roof-tiles-products-chennai.html" },
    { "source": "/cool-roof-tiles-gallery-chennai", "destination": "/cool-roof-tiles-gallery-chennai.html" },
    { "source": "/contact-cool-roof-tiles-dealer-chennai", "destination": "/contact-cool-roof-tiles-dealer-chennai.html" },
    { "source": "/cool-roof-tiles-in-chennai", "destination": "/cool-roof-tiles-in-chennai.html" },
    { "source": "/cool-roof-tiles-price-chennai", "destination": "/cool-roof-tiles-price-chennai.html" },
    { "source": "/affordable-cool-roof-tiles-price-chennai", "destination": "/cool-roof-tiles-price-chennai.html" },
    { "source": "/cooling-tiles-in-chennai", "destination": "/cooling-tiles-in-chennai.html" },
    { "source": "/cooling-tiles-price-in-chennai", "destination": "/cooling-tiles-price-in-chennai.html" },
    { "source": "/clay-roof-tiles-in-chennai", "destination": "/clay-roof-tiles-in-chennai.html" },
    { "source": "/ceramic-white-cool-roof-tile-chennai", "destination": "/ceramic-white-cool-roof-tile-chennai.html" },
    { "source": "/ceramic-cool-roof-tiles-price-chennai", "destination": "/ceramic-cool-roof-tiles-price-chennai.html" },
    { "source": "/heat-resistant-ceramic-terrace-tiles-chennai", "destination": "/heat-resistant-ceramic-terrace-tiles-chennai.html" },
    { "source": "/vitrified-cool-roof-tile-blue-chennai", "destination": "/vitrified-cool-roof-tile-blue-chennai.html" },
    { "source": "/vitrified-cool-roof-tile-ivory-chennai", "destination": "/vitrified-cool-roof-tile-ivory-chennai.html" },
    { "source": "/vitrified-cool-roof-tile-pink-chennai", "destination": "/vitrified-cool-roof-tile-pink-chennai.html" },
    { "source": "/vitrified-cool-roof-tile-almond-chennai", "destination": "/vitrified-cool-roof-tile-almond-chennai.html" },
    { "source": "/terrace-premium-rough-grout-chennai", "destination": "/terrace-premium-rough-grout-chennai.html" },
    { "source": "/waterproofing-grout-admixture-chennai", "destination": "/waterproofing-grout-admixture-chennai.html" },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
